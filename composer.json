{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"license": "MIT",
	"require": {
		"laravel/framework": "4.1.*",
		"way/generators": "dev-master",
		"laravelbook/ardent": "dev-master",
		"anahkiasen/former": "dev-master",
		"patricktalmadge/bootstrapper": "dev-develop",
		"emberlabs/gravatarlib": "dev-master@dev",
		"thomaswelton/laravel-gravatar": "dev-master",
		"kriswallsmith/assetic": "1.1.*@dev",
		"leafo/lessphp": "0.4.*@dev",
		"ptachoire/cssembed": "dev-master",
		"patchwork/jsqueeze": "dev-master",
		"slushie/laravel-assetic": "dev-master",
		"davejamesmiller/laravel-breadcrumbs": "dev-master",
		"intervention/image": "dev-master",
		"intervention/imagecache": "dev-master",
		"kmd/logviewer": "1.1.*",
		"machuga/authority-l4": "2.1.*@dev",
		"cviebrock/eloquent-sluggable": "dev-develop",
		"erusev/parsedown": "dev-master",
		"endroid/qrcode": "1.*@dev",
		"sabre/vobject": "3.2.*@dev"
		
	},
	"autoload": {
		"classmap": [
			"app/commands",
			"app/controllers",
			"app/models",
			"app/database/migrations",
			"app/database/seeds",
			"app/tests/TestCase.php"
		],
		"psr-0": {
            "HMIF\\": "app",
            "Felixkiss\\SlugRoutes\\": "app/felixkiss/slug-routes/src"
        }
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "stable"
}
