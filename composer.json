{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"license": "MIT",
	"require": {
		"laravel/framework": "4.1.*",
		
		"machuga/authority-l4": "2.1.*@dev",
		"ollieread/multiauth": "dev-master",

		"laravelbook/ardent": "dev-master",
		"fitztrev/laravel-html-minify": "dev-master",
		"cviebrock/eloquent-sluggable": "dev-develop",
		"davejamesmiller/laravel-aliases": "dev-master",

		"kmd/logviewer": "1.1.*",

		"anahkiasen/former": "dev-master",
		"patricktalmadge/bootstrapper": "dev-develop",
		"davejamesmiller/laravel-breadcrumbs": "dev-master",
		
		"maatwebsite/excel": "1.*",
		"endroid/qrcode": "1.*@dev",
		"thujohn/pdf": "dev-master",
		"sabre/vobject": "3.2.*@dev",
		"intervention/image": "1.6.2",
		"erusev/parsedown": "dev-master",

		"vinicius73/seotools": "dev-master",
		"diego1araujo/titleasy": "dev-master",
		
		"embed/embed": "dev-master",
		"greggilbert/recaptcha": "dev-master",
		"thomaswelton/laravel-gravatar": "dev-master"
	},
	"autoload": {
		"classmap": [
			"app/commands",
			"app/controllers",
			"app/models",
			"app/database/migrations",
			"app/database/seeds",
			"app/tests/TestCase.php"
		],
		"psr-0": {
            "HMIF\\": "app",
            "Felixkiss\\SlugRoutes\\": "app/felixkiss/slug-routes/src",
            "Qwildz": "app/packages/qwildz/core/src"
        },
		"psr-4": {
            "PHPZip\\Zip\\": "app/packages/phpzip/phpzip/src/Zip"
        }
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "stable"
}
